{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "grocery": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/grocery",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": [
             "src/assets/libs/jquery/jquery.js",
              "src/assets/libs/bootstrap/js/bootstrap.js",
              "src/assets/libs/jquery.countdown/jquery.countdown.js",
              "src/assets/libs/nivo-slider/js/jquery.nivo.slider.js",
              "src/assets/libs/owl.carousel/owl.carousel.min.js",
              "src/assets/libs/slider-range/js/tmpl.js",
              "src/assets/libs/slider-range/js/jquery.dependClass-0.1.js",
              "src/assets/libs/slider-range/js/draggable-0.1.js",
              "src/assets/libs/slider-range/js/jquery.slider.js",
              "src/assets/libs/elevatezoom/jquery.elevatezoom.js",
              "src/assets/js/main.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "grocery:build:production"
            },
            "development": {
              "buildTarget": "grocery:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": [
              "../libs/jquery/jquery.js",
              "../libs/bootstrap/js/bootstrap.js",
              "../libs/jquery.countdown/jquery.countdown.js",
              "../libs/nivo-slider/js/jquery.nivo.slider.js",
              "../libs/owl.carousel/owl.carousel.min.js",
              "../libs/slider-range/js/tmpl.js",
              "../libs/slider-range/js/jquery.dependClass-0.1.js",
              "../libs/slider-range/js/draggable-0.1.js",
              "../libs/slider-range/js/jquery.slider.js",
              "../libs/elevatezoom/jquery.elevatezoom.js",
              "../js/main.js"
            ]
          }
        }
      }
    }
  }
}
